
{
    "$schema": "http://json.schemastore.org/prettierrc",
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "importOrder": [
      "",
      "<THIRD_PARTY_MODULES>",
      "<BUILT_IN_MODULES>",
      "^@/",
      "^~/",
      "^[.]"
    ],
    "jsxSingleQuote": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }